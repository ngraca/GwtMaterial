<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:p="urn:import:gwt.material.design.demo.client.panel">
	<ui:with field='res' type='gwt.material.design.demo.client.resources.MaterialResources' />
	<ui:style>
		body{
			background: #e7e7e7;
		}
		.button{
			vertical-align: top;
			margin-left: 20px;
		}
		.panel{
			padding-left: 5%;
			width: 100%;
			margin-top: -150px;
			padding: 10px 50px 50px 50px;
			background: #FFF;
		}
		.pointer{
			cursor: pointer;
		}
		header, main, footer {
		    padding-left: 240px;
		}
		.logo{
			padding: 10px;
			border-bottom: 1px solid #E9E9E9;
		}
	</ui:style>	
	<g:HTMLPanel >
			<main>
				<m:MaterialNavBar wave="blue" ui:field="navBar" name="myNavBar"  sideBar="fixed" color="blue darken-4"  align="right" >
					<m:sidebaritem><m:MaterialImage resource="{res.ic_gwt_logo}" addStyleNames="{style.logo}"/></m:sidebaritem>
					<m:sidebaritem><m:MaterialLink ui:field="btnAbout" text="About" textColor="blue"  /></m:sidebaritem>
					<m:sidebaritem><m:MaterialLink ui:field="btnButtons" text="Buttons" textColor="blue"/></m:sidebaritem>
					<m:sidebaritem><m:MaterialLink ui:field="btnForms" text="Forms" textColor="blue"/></m:sidebaritem>
					<m:sidebaritem><m:MaterialLink ui:field="btnDialogs" text="Dialogs" textColor="blue"/></m:sidebaritem>
					<m:sidebaritem><m:MaterialLink ui:field="btnLoaders" text="Loaders" textColor="blue"/></m:sidebaritem>
					<m:sidebaritem><m:MaterialLink ui:field="btnMedia" text="Media" textColor="blue"/></m:sidebaritem>
					<m:sidebaritem><m:MaterialLink ui:field="btnCards" text="Cards" textColor="blue"/></m:sidebaritem>
					<m:sidebaritem><m:MaterialLink ui:field="btnNavigations" text="NavBar" textColor="blue"/></m:sidebaritem>
					<m:sidebaritem><m:MaterialLink ui:field="btnCollapsibles" text="Collapsible" textColor="blue"/></m:sidebaritem>
					<m:sidebaritem><m:MaterialLink ui:field="btnMaterialDropDowns" text="Dropdown" textColor="blue"/></m:sidebaritem>
					<m:sidebaritem><m:MaterialLink ui:field="btnMaterialFooter" text="Footer" textColor="blue"/></m:sidebaritem>
					<m:sidebaritem><m:MaterialLink ui:field="btnMaterialTabs" text="Tabs" textColor="blue"/></m:sidebaritem>
					<m:sidebaritem><m:MaterialLink ui:field="btnCollections" text="Collections" textColor="blue"/></m:sidebaritem>
				</m:MaterialNavBar>
				<m:MaterialTopNav resource="{res.bg_top_nav}" ui:field="topNav"  color="blue" textColor="white">
					<m:child>
						<m:MaterialButton ui:field="btnGettingStarted" text="Get Started" size="large" color="blue darken-2" />
					</m:child>
						
				</m:MaterialTopNav>
				<g:HTMLPanel ui:field="contentPanel" addStyleNames="{style.panel} container z-depth-2">
					<p:MaterialGettingStarted ui:field="materialGettingStarted"/>
					<p:MaterialHome ui:field="materialHome"/>
					<p:MaterialStyle ui:field="materialStyle"/>
					<p:MaterialButtonPanel ui:field="materialButtons"/>
					<p:MaterialForms ui:field="materialForms"/>
					<p:MaterialDialogs ui:field="materialDialogs"/>
					<p:MaterialLoaders ui:field="materialLoaders"/>
					<p:MaterialMedia ui:field="materialMedia"/>
					<p:MaterialCard ui:field="materialCards"/>
					<p:MaterialNavigations ui:field="materialNavigations"/>
					<p:MaterialCollapsibles ui:field="materialCollapsibles"/>
					<p:MaterialDropDowns ui:field="materialDropDowns"/>
					<p:MaterialFooterPanel ui:field="materialFooters"/>
					<p:MaterialTabsPanel ui:field="materialTabs"/>
					<p:MaterialCollectionPanel ui:field="materialCollections"/>
					<!-- <p:Team /> -->
				</g:HTMLPanel>
			</main>	
			
		
	</g:HTMLPanel>
</ui:UiBinder> 